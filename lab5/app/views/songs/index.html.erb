<h1>Songs</h1>

<%= link_to "New Song", new_song_path %>

<ul>
  <% @songs.each do |song| %>
    <li>
      <%= song.title %>
      <%= link_to "Show", song_path(song) %>
      <%= link_to "Edit", edit_song_path(song) %>
      <%= link_to "Delete", song_path(song), method: :delete, data: { confirm: "Are you sure?" } %>
    </li>
  <% end %>
</ul>

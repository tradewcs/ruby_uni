<h1>Artists</h1>

<%= link_to 'Add New Artist', new_artist_path %>

<ul>
  <% @artists.each do |artist| %>
    <li>
      <%= artist.name %>
      <%= link_to 'Edit', edit_artist_path(artist) %>
      <%= link_to 'Delete', artist_path(artist), method: :delete, data: { confirm: 'Are you sure?' } %>
    </li>
  <% end %>
</ul>

<%= link_to 'Home', songs_path%>

